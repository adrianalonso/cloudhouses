<!DOCTYPE html>
<html class="no-js" lang="{{ app.request.locale }}">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,600" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,700" rel="stylesheet">
    <link href="https://use.fontawesome.com/releases/v5.0.1/css/all.css" rel="stylesheet">
    <link href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css" rel="stylesheet"
          integrity="sha512-Rksm5RenBEKSKFjgI3a41vrjkw4EVPlJ3+OiI65vTjIdo9brlAacEuKOiQ5OFh7cOI1bkDwLqdLw3Zg0cRJAAQ=="
          crossorigin=""/>

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">

    <!-- Compiled and minified JavaScript -->

    {% block page_head %}
        <title>{{ title|default('') }} - Slackelpuff Website</title>
    {% endblock %}

    {% block head_styles %}
        {{ encore_entry_link_tags('slackelpuff') }}
    {% endblock %}

</head>
<body>
{% block header %}
    <nav class="yellow darken-3" role="navigation">
        <div class="nav-wrapper container">
            <a id="logo-container" href="{{ path( ezpublish.rootLocation ) }}" class="brand-logo center">
                Slackelpuff
            </a>
            <ul id="nav-mobile" class="left hide-on-med-and-down">
                <li><a href="/"> Home</a></li>
            </ul>
        </div>
    </nav>
{% endblock %}

{% block content %}
{% endblock %}

{% block footer %}

{% endblock %}

{% block javascripts %}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

{% endblock %}
</body>